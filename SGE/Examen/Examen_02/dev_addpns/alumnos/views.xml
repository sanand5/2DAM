<odoo>
  <data>
    <!-- explicit list view definition -->

    <record model="ir.ui.view" id="alumnos.alumn_list">
      <field name="name">alumnos alumn list</field>
      <field name="model">alumnos.alumn</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
        </tree>
      </field>
    </record>

    <!-- actions opening views on models -->

    <record model="ir.actions.act_window" id="alumnos.action_alumn_window">
      <field name="name">alumnos alumn window</field>
      <field name="res_model">alumnos.alumn</field>
      <field name="view_mode">tree,form</field>
    </record>


    <!-- server action to the one above -->
<!--
    <record model="ir.actions.server" id="alumnos.action_server">
      <field name="name">alumnos server</field>
      <field name="model_id" ref="model_alumnos_alumnos"/>
      <field name="state">code</field>
      <field name="code">
        action = {
          "type": "ir.actions.act_window",
          "view_mode": "tree,form",
          "res_model": model._name,
        }
      </field>
    </record>
-->

    <!-- Top menu item -->
    
    <menuitem name="Alumnos" id="alumnos.menu_root"/>

    <!-- menu categories -->
    <menuitem name="Menu 1" id="alumnos.menu_1" parent="alumnos.menu_root"/>
    <!--
    <menuitem name="Menu 2" id="alumnos.menu_2" parent="alumnos.menu_root"/>
-->
    <!-- actions -->
    <menuitem name="Listado" id="alumnos.menu_1_alumn_list" parent="alumnos.menu_1"
    action="alumnos.action_alumn_window"/>
    <!--
    <menuitem name="Server to list" id="alumnos" parent="alumnos.menu_2"
              action="alumnos.action_server"/>
-->
  </data>
</odoo>